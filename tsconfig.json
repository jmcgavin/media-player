{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "baseUrl": ".",
    "moduleResolution": "bundler",
    "resolveJsonModule": true
  },
  "exclude": ["forge.config.js"]
}
